<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Exclude generated code (OpenAPI generated code) -->
    <Match>
        <Package name="~.*\.generated\..*"/>
    </Match>
    <Match>
        <Package name="com.bank.paymentinitiation.generated"/>
    </Match>
    
    <!-- Exclude MapStruct generated implementations -->
    <Match>
        <Class name="~.*MapperImpl"/>
    </Match>
    
    <!-- Exclude JPA Entities -->
    <Match>
        <Class name="com.bank.paymentinitiation.adapter.out.persistence.entity.PaymentOrderEntity"/>
    </Match>
    
    <!-- Exclude main application class -->
    <Match>
        <Class name="com.bank.paymentinitiation.PaymentInitiationServiceApplication"/>
    </Match>
</FindBugsFilter>
